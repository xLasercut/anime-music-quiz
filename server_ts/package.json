{
  "name": "anime-music-quiz-server",
  "version": "1.0.0",
  "description": "anime music quiz server",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc -b",
    "compile:watch": "tsc -w",
    "start:watch": "nodemon dist/index.js",
    "serve": "concurrently --kill-others \"npm run compile:watch\" \"npm run start:watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xLasercut/anime-music-quiz.git"
  },
  "author": "xLasercut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xLasercut/anime-music-quiz/issues"
  },
  "homepage": "https://github.com/xLasercut/anime-music-quiz#readme",
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/ini": "^1.3.30",
    "@types/mustache": "^0.8.32",
    "@types/q": "^1.5.2",
    "@types/socket.io": "^2.1.4",
    "express": "^4.17.1",
    "ini": "^1.3.5",
    "mustache": "^3.1.0",
    "q": "^1.5.1",
    "socket.io": "^2.3.0",
    "typescript": "^3.6.4",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "nodemon": "^1.19.4"
  }
