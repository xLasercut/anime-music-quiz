{
  "name": "anime-music-quiz-server",
  "version": "1.0.0",
  "description": "Server for anime music quiz game",
  "main": "index.coffee",
  "scripts": {
    "start": "coffee index.coffee",
    "generate": "coffee tools/raw-list-generator.coffee",
    "process": "coffee tools/full-list-generator.coffee && coffee tools/choices-generator.coffee",
    "update": "coffee tools/user-list-updater.coffee",
    "start:test": "cross-env NODE_ENV=test npm run start",
    "test": "coffee ./database/database.coffee"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xLasercut/anime-music-quiz.git"
  },
  "author": "xLasercut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xLasercut/anime-music-quiz/issues"
  },
  "homepage": "https://github.com/xLasercut/anime-music-quiz#readme",
  "dependencies": {
    "coffeescript": "^2.4.1",
    "express": "^4.17.1",
    "ini": "^1.3.5",
    "mustache": "^3.1.0",
    "q": "^1.5.1",
    "socket.io": "^2.3.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
